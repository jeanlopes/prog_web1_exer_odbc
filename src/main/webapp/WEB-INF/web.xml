</env-entry>
<env-entry> 
    <env-entry-name>host</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value>jdbc:oracle:thin:@oracle.inf.poa.ifrs.edu.br:1521:XE</env-entry-value> 
</env-entry>
<env-entry> 
    <env-entry-name>user</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value>karen</env-entry-value> 
</env-entry>

<env-entry> 
    <env-entry-name>password</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value>karen</env-entry-value> 
</env-entry>
</web-app>